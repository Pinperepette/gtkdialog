#!/bin/sh

GTKDIALOG=gtkdialog

function print_this() {
  echo "print: $1"
}; export -f print_this

export MAIN_DIALOG='
<vbox>
  <button>
    <label>function</label>
    <action>print_this button</action>
  </button>
  <button>
    <label>Exit</label>
  </button>
</vbox>
'

$GTKDIALOG --program=MAIN_DIALOG
